{
  "name": "@arturinspector/tracex-logger",
  "version": "1.0.2",
  "description": "distributed tracing SDK for x402 payment operations",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=performance",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tracing",
    "distributed-tracing",
    "observability",
    "x402",
    "solana",
    "payment",
    "monitoring",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
